host: satel.honolulu.org
port: 7094
code: {{ satel.code }}
partitions:

{%- for partition in satel.partitions %}

  {{ partition.id }}:
    name: {{ partition.name }}
    arm_home_mode: {{ partition.arm_home_mode }}
{% endfor %}

zones:

{%- for zone in satel.zones %}

  {{ zone.id }}:
    name: {{ zone.name }}
{% endfor %}

switchable_outputs:

{%- for switchable_output in satel.switchable_outputs %}

  {{ switchable_output.id }}:
    name: {{ switchable_output.name }}
{% endfor %}
